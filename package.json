{
  "name": "@cobaltio/cobalt",
  "version": "1.3.0",
  "description": "SDK from Cobalt to access integration apis",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "scripts": {
    "docs": "npx typedoc src/index.ts src/Apis/types.ts",
    "build": "microbundle --tsconfig tsconfig.json --no-sourcemap",
    "dev": "microbundle --watch --tsconfig tsconfig.json --no-sourcemap",
    "test": "npm run build && echo \"TEST DONE\""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "Cobalt",
    "Cobalt-SDK",
    "Process Automation",
    "Typescript",
    "api",
    "SDK",
    "Cobalt",
    "cobalt",
    "cobalt-sdk"
  ],
  "author": "Subrat",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "microbundle": "^0.15.1",
    "prettier": "^2.7.1",
    "typedoc": "^0.24.8",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "axios": "^1.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gocobalt/cobalt-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/gocobalt/cobalt-sdk/issues"
  },
  "homepage": "https://gocobalt.github.io/cobalt-sdk/"
